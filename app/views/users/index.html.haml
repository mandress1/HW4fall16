-# This files is app/views/users/list.html.haml
%h1 All Users

%table#users
  %thead
    %tr
      %th User ID
      %th User E-Mail
  %tbody
    - @users.each do |user|
      %tr
        %td= "id: #{user}"
        %td= "Email: #{user.email}"
        
%p "#{User.column_names}"
  
= link_to 'home', movies_path